{
  "name": "infinito-web-parcel",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "parcel src/index.html --open --no-cache",
    "clean": "rm -rf dist/*",
    "build:parcel": "parcel build src/index.html --no-content-hash --no-source-maps --public-url ./",
    "build": "npm run clean && npm run build:parcel",
    "sass:watch": "node-sass --watch --include-path sass ./src/scss/index.scss ./src/css/styles.css ",
    "sass:compile": "node-sass --include-path sass ./src/scss/index.scss ./dist/css/styles.css "
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/codrops/[NAME].git"
  },
  "keywords": [],
  "author": "Codrops",
  "license": "MIT",
  "homepage": "https://tympanus.net/codrops",
  "bugs": {
    "url": "https://github.com/codrops/[NAME]/issues"
  },
  "devDependencies": {
    "parcel": "^2.8.2"
  },
  "dependencies": {
    "node-sass": "7.0.3",
    "@studio-freight/lenis": "^0.2.28",
    "gsap": "^3.11.4",
    "splitting": "^1.0.6"
  }
}
